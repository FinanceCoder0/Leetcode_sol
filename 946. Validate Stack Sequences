# https://leetcode.com/problems/validate-stack-sequences/submissions/1302611659

class Solution(object):
    def validateStackSequences(self, pushed, popped):
        stack = []
        j = 0 
        for num in pushed:
            stack.append(num) 
            while stack and stack[-1] == popped[j]:
                stack.pop()
                j += 1
        return j == len(popped)
        
